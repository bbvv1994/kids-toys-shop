{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://kids-toys-backend.onrender.com/api/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "https://kids-toys-backend.onrender.com/uploads/$1"
    }
  ],
  "env": {
    "REACT_APP_API_BASE_URL": "https://kids-toys-backend.onrender.com"
  }
} 